Ref: "genero"."id_genero" < "socio_genero"."id_genero"

Ref: "socio"."id_socio" < "socio_actores"."id_socio"

Ref: "actor"."id_actor" < "socio_actores"."id_actor"

Ref: "socio"."id_socio" < "socio_directores"."id_socio"



