Table peliculas {
  titulo text
  id_peli int [primary key]
}

Table socio {
  id_socio int [primary key]
  nombre text
  direccion text
  telefono int
}

Table genero  {
  id_genero int [primary key]
  genero text
}

Table director  {
  id_director int [primary key]
  director text
}

Table actor {
  id_actor int [primary key]
  actor text
}

Table peli_genero {
  id_peli int
  id_genero int
}

Table peli_directores {
  id_peli int
  id_director int
}

table peli_actores {
  id_peli int
  id_actor int
}

Table socio_genero {
  id_socio int
  id_genero int
}

Table socio_directores {
  id_socio int
  id_director int
}

table socio_actores {
  id_socio int
  id_actor int
}

Table cintas {
  id_cinta int [primary key]
  id_peli int
  fecha date
}

Table renta_cinta {
  id_cinta int
  id_socio int
}

Table lista_de_espera {
  id_peli int
  id_socio int
}

